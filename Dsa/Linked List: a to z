// Creating a linked list

#include<bits/stdc++.h>
using namespace std;

struct node
{
    node *next;
    int data;
    node(int data)
    {
        this->data = data;
        next = NULL;
    }
};

int main()
{
    node *head = new node(5);
    node *n1 = new node(10);
    node *n2 = new node(15);
    head->next = n1;
    n1->next = n2;
    n2->next = NULL;
    
    // Traversing a Linked List
    node a = *head;
    while(a.next!=NULL)
    {
        cout<<a.data<<" ";
        a = *(a.next);
    } cout<<a.data;
    
return 0;
}
